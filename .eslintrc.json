{
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "process": "readonly",
        "Set": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": ["warn", { "allow": ["warn", "error"]}],
        "no-undef": "warn"
    }
}